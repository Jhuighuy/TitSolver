{
  "configurations": [
    /*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
    {
      "name": "macOS",
      "includePath": [
        "/opt/homebrew/include",
        "${workspaceFolder}/source/**",
        "${workspaceFolder}/output/cmake_output/**"
      ],
      "defines": [
        "__INTELLISENSE__"
      ],
      "compilerPath": "/opt/homebrew/bin/g++-13",
      "cStandard": "c11",
      "cppStandard": "c++23",
      "intelliSenseMode": "macos-gcc-arm64"
    },
    /*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
    {
      "name": "Windows (MSYS2)",
      "includePath": [
        "C:/msys64/mingw64/include/**",
        "${workspaceFolder}/source/**",
        "${workspaceFolder}/output/cmake_output/**"
      ],
      "defines": [
        "__AVX__",
        "__AVX512F__",
        "__INTELLISENSE__"
      ],
      "compilerPath": "C:/msys64/mingw64/bin/g++.exe",
      "cStandard": "c11",
      "cppStandard": "c++23",
      "intelliSenseMode": "windows-gcc-x64"
    }
    /*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
  ],
  "version": 4
}
