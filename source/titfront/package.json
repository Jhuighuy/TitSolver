    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@eslint/js": "^9.38.0",
    "@tailwindcss/vite": "4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/http-proxy": "^1.17.16",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.8.1",
